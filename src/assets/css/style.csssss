/******************************************************************login page css starts here***********************************************/
section.login-section {
    height: 100%;
    width: 100%;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

section.login-section:after {
    position: absolute;
    content: "";
    left: 0;
    right: 0;
    top: 0;
    height: 100%;
    width: 100%;
    background: rgba(0, 51, 95, 0.9);
    z-index: 0;
}

.login-form {
    width: 100%;
    max-width: 420px;
    margin: auto;
    position: relative;
    z-index: 2;
    text-align: center;
}

a.logo-img {
    display: inline-block;
    margin: 0 0 45px;
}
.login-form form {
    position: relative;
    z-index: 5;
}
.login-form .form-group {
    margin-bottom: 24px;
    position: relative;
}
.form-group i {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    left: 15px;
    color: #fff;
}
.login-form input.form-control {
    background: rgba(255, 255, 255, 0.3);
    height: 55px;
    border: 0;
    border-radius: 30px;
    color: #fff;
    padding: 0 30px 0 50px;
    opacity: 1;
}

.login-form input.form-control:focus {
    box-shadow: 0 0 0 0.2rem rgba(255, 255, 255, 1)
}

.login-form input[type="submit"] {
    background: #0f4a7d;
    width: 100%;
    height: 55px;
    border: 0;
    border-radius: 30px;
    text-transform: uppercase;
    font-weight: 500;
    font-size: 17px;
    font-family: 'Roboto', sans-serif;
    color: #fff;
    cursor: pointer;
    box-shadow: 0 0 10px rgba(7, 29, 47, 0.64);
    transition: all 0.3s ease-in-out;
    -webkit-transition: all 0.3s ease-in-out;
}
.login-form input[type="submit"]:hover{letter-spacing: 2px;}
.login-form input::-webkit-input-placeholder {
    color: #fff;
    opacity: 1
}

.login-form input::-moz-placeholder {
    color: #fff;
    opacity: 1
}

.login-form input:-ms-input-placeholder {
    color: #fff;
    opacity: 1
}

.login-form input::-ms-input-placeholder {
    color: #fff;
    opacity: 1
}

.login-form input::placeholder {
    color: #fff;
    opacity: 1
}

/*****************************************************media queries starts here****************************************/

@media screen and (max-width: 1500px) {
    .listing small {
        width: 135px;
        font-size: 14px !important;
    }

    .main-content .table thead th {
        font-size: 12px;
    }
}
@media screen and (max-width: 1400px) {
    .version-second .button-set a{font-size: 12px;}
    .bottom-part .button-set a {
    width: 135px;
    margin: 0 5px 0;
        font-size: 12px;
}
    .version-third .card-body.cmpny-dtls .right-bar {
    width: 35%;
}
   .version-third .card-body.cmpny-dtls .left-bar {
    width: 64%;
}
}

