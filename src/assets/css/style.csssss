@font-face {
    font-family: 'ArialRoundedMTStd';
    src: url("../fonts/ArialRoundedMTStd.eot?#iefix") format("embedded-opentype"), url("../fonts/ArialRoundedMTStd.otf") format("opentype"), url("../fonts/ArialRoundedMTStd.woff") format("woff"), url("../fonts/ArialRoundedMTStd.ttf") format("truetype"), url("../fonts/ArialRoundedMTStd.svg#ArialRoundedMTStd") format("svg");
    font-weight: normal;
    font-style: normal;
