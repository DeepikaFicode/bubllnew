.user-table td:first-child, .user-table th:first-child {
    display: none;
}
*/
a.add-btn {
    color: #00335f;
    font-size: 15px;
    line-height: 0;
}

.card-body.cmpny-dtls {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    flex-direction: row;
    flex: inherit;
    border-top: 1px solid rgba(76, 76, 76, 0.37);
    display: none;
}

.listing {
    display: inline-block;
    margin: 0 0 10px;
    width: 48%;
}

.button-set {
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
    margin: 0 0 5px;
}

.button-set a {
    background: #00335f;
    color: #fff;
    width: 48%;
    margin: 0 0 9px;
    text-align: center;
    padding: 10px 5px;
    border-radius: 5px;
    font-weight: 500;
    font-size: 14px;
    transition: all 0.3s ease-in-out;
    -webkit-transition: all 0.3s ease-in-out;
}

.button-set a:hover {
    background: #2a58ad;
    color: #fff;
    text-decoration: none;
}

.map-box iframe {
    width: 100%;
    height: 180px;
}

.btn-secondary:hover {
    color: #fff;
    background-color: #6c757d;
    border-color: #666e76;
    box-shadow: 0 0 0 0.2rem rgba(134, 142, 150, .5);
}

.card-body.cmpny-dtls .left-bar {
    width: 58%;
    text-align: left;
    display: inline-block;
    vertical-align: top;
}

.card-body.cmpny-dtls .right-bar {
    width: 40%;
    display: inline-block;
    vertical-align: top;
}

.adress-info {
    width: 100%;
    margin: 20px 0 0;
}

p.expand {
    text-align: center;
    padding: 0 15px;
}

.expand a {
    color: #000;
    font-size: 16px;
    text-transform: capitalize;
    text-decoration: none;
}

.expand a i {
    display: block;
}

.map-box {
    margin: 0 0 10px;
}

.card.mb-3 {
    max-width: 100% !important;
}

.info-box .card.mb-3 {
    box-shadow: 0 0 3px #b3b4b5;
}

