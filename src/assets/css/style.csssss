/****************************************************Table css*****************************************************/

.table tr {
    border-top: 1px solid #bfbfbf;
}

table th,
table td {
    text-align: center;
    border: 0 !important;
    color: #000;
    font-size: 13px;
    text-align: left;
    padding: .75rem .25rem;
}

table tr:nth-child(even) {
    background-color: transparent
}

.table tr:hover {
    background-color: rgba(255, 255, 255, 0.1) !important;
}

th {
    background: #333;
    color: #fff;
}

.pagination {
    margin: 0;
}

.pagination li:hover {
    cursor: pointer;
}

.header_wrap {
    padding: 30px 0;
}

.num_rows {
    width: 20%;
    float: left;
}

.tb_search {
    width: 20%;
    float: right;
}

.pagination-container {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.pagination li {
    display: inline-block;
    background: #1d233c;
    padding: 5px 15px;
    margin: 0 3px 0 0;
    border-radius: 3px;
    color: #fff;
}

.pagination li.active,
