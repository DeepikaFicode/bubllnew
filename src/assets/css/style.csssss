    background: #2a58ad;
}

.table td a.btn {
    background: #2a58ad;
    color: #fff;
    text-transform: capitalize;
    padding: 5px 10px;
    font-size: 12px;
    display: inline;
}

.table td a.btn:hover {
    background: #00335f;
    color: #fff;
    text-decoration: none;
}

.about-content {
    color: #000;
    font-size: 14px;
    margin: 0;
    display: block;
    line-height: 1.7;
}

.company-tables {
    margin: 30px 0 0;
}

.listing small {
    font-size: 85% !important;
    width: 170px;
    display: inline-block;
    vertical-align: middle;
    font-weight: 600;
}

.top-section {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.top-section .m-t-30.text-center {
    margin: 0 48px 0 0;
}
