/*---------------------------------------------------------------------
- Reset
--------------------------------------------------------------------*/
* {
    margin: 0px;
    padding: 0px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

::-webkit-scrollbar {
    min-width: 3px;
    width: 0.3%;
    max-width: 4px;
}

::-webkit-scrollbar,
::-webkit-scrollbar-track,
::-webkit-scrollbar-track-piece {
    background: #171719;
}

::-webkit-scrollbar-button,
::-webkit-scrollbar-thumb {
    background: rgba(255, 255, 255, 0.6);
}

.preloader {
    background: #fff none repeat scroll 0 0;
    bottom: 0;
    left: 0;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 999999;
}

.preloader > span {
    display: inline-block;
    font-size: 24px;
    height: 200px;
    left: 50%;
    line-height: 200px;
    position: absolute;
    margin-left: -100px;
    margin-top: -100px;
    top: 50%;
    width: 200px;
    color: #000;
    text-align: center;
    overflow: hidden;
}

.preloader > span img {
    width: 150px;
    height: 150px;
}

body,
html {
    height: 100%;
    font-family: Poppins, sans-serif !important;
    font-style: normal;
}

body {
    background: url(../images/main-bg.png) no-repeat center top;
    background-size: cover;
    position: relative;
}

canvas#bubble {
    position: absolute;
    z-index: 0;
    width: 100%;
    height: 100%;
}

.content-wrapper {
   background: rgba(255, 255, 255, 0.88);
}

.navbar-nav .nav-link i {
    position: relative;
    top: -3px;
}

.page-head {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    align-items: center;
    margin: 0 0 15px;
}

.page-head h2 {
    color: #000;
    font-size: 20px;
    text-transform: capitalize;
}

.page-head ul li {
    list-style: none;
    display: inline;
}

.page-head ul li + li {
    padding-left: .5rem;
}

.page-head ul li + li:before {
    content: "\f105";
    font-family: 'fontAwesome';
    color: #6a6b6b;
    font-size: 13px;
    padding-right: .5rem;
}

.page-head ul li a {
    color: #000;
    font-size: 12px;
}

.page-head ul li {
    color: #353535;
    font-size: 12px;
}

.content-boxes {}

.card {
    background-color: rgba(255, 255, 255, 0.47);
}

.card-body h5.card-title {
    color: #000;
}

.card-body .card-text {
    color: #000;
}

.card-group .col-md-4 {
    justify-content: center;
    align-items: center;
    display: flex;
    border-right: 1px solid #00335f;
    background: #00335f;

}

.info-box .card i {
    color: #fff;
    font-size: 50px;
}

ul.navbar-nav.navbar-nav-right {
    z-index: 2;
}

.user-profile {
    padding: 15px;
    text-align: center;
    display: none;
}

.user-profile .user-pic img {
    width: 60px;
}

.user-profile .user-name {
    color: #fff;
    position: relative;
    white-space: nowrap;
    margin: 10px 0 0;
}

.card .no-gutters {
    height: 100%;
}

.card-body h6 {
    color: #000;
    font-size: 13px;
    width: 45%;
    display: inline-block;
    vertical-align: middle;
    margin: 0;
}

h4.card-title.m-t-10 {
    color: #fff;
    margin: 10px 0;
}

.btn-circle {
    border-radius: 100%;
    width: 40px;
    height: 40px;
    padding: 10px;
}

.btn-secondary {
    color: #fff;
    background-color: #6c757d;
    border-color: #6c757d;
}

.right-bar small.text-muted.p-t-30.db {
    vertical-align: middle;
    display: inline-block;
    margin: 0;
}

.profile-view .card-body button.btn {
    margin: 0 5px 0;
    cursor: pointer;
}

.main-content {
    margin: 0 0 15px;
}

