    border-radius: 5px;
    overflow: hidden;
    overflow-x: auto;
}

.main-content .table thead th {
    vertical-align: bottom;
    border: 0;
    background: transparent;
    padding: 15px 0.25rem;
}

select#maxRows {
    background: #131633;
    border: 0;
    color: #fff;
}

.tb_search input {
    background: #131633;
    border: 0;
    color: #fff;
}

.top-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 0 0 15px;
}

.top-header h3 {
    color: #000;
    font-size: 17px;
}

.top-header a {
    color: #000;
    background: #161934;
    padding: 10px 20px;
    box-shadow: 0 0 10px #4d4f63;
